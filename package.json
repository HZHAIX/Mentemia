{
  "name": "Mentemia",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write 'src/**/*.tsx' && prettier --write 'src/**/*.ts'",
    "format-check": "prettier --check 'src/**/*.tsx' && prettier --check  'src/**/*.ts'",
    "clean": "watchman watch-del-all && rm -rf node_modules $TMPDIR/metro-* $TMPDIR/haste-map-*",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.3.4",
    "@react-navigation/native": "^5.2.4",
    "@react-navigation/stack": "^5.2.19",
    "lodash": "^4.17.15",
    "ramda": "^0.27.0",
    "react": "16.11.0",
    "react-native": "^0.62.2",
    "react-native-animatable": "^1.3.3",
    "react-native-flipper": "^0.39.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.7.0",
    "react-native-tab-view": "^2.14.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^9.4.0",
    "react-navigation": "^4.3.9",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-flipper": "^1.0.0",
    "redux-observable": "^1.2.0",
    "remove": "^0.1.5",
    "rxjs": "^6.5.5",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/react": "^16.9.34",
    "@types/react-native": "^0.62.7",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-redux": "^7.1.8",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-jest": "^26.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "jest": "^26.0.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.5",
    "react-native-testing-library": "^1.13.2",
    "react-test-renderer": "16.11.0",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn format-check"
    }
  }
}
